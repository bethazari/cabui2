{
  "env": {
    "test": {
      "presets": [
        "es2015",
        "react",
        "stage-1"
      ]
    },
    "production": {
      "presets": [
        ["es2015", { "modules": false }],
        "react",
        "stage-1"
      ]
    },
    "development": {
      "presets": [
        ["es2015", { "modules": false }],
        "react",
        "stage-1"
      ]
    }
  },
  "plugins": [
    "react-hot-loader/babel",
    "transform-runtime"
  ]
}